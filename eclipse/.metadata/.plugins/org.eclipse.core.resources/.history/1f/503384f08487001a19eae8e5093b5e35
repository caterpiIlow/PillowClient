package caterpillow.Module.Specific;

import org.lwjgl.input.Keyboard;
import org.lwjgl.input.Mouse;

import caterpillow.Module.Category;
import caterpillow.Module.Module;

public class MouseSwap extends Module {
	public MouseSwap() {
		super("MouseSwap", Keyboard.KEY_K, Category.MISC);
	}

	@Override
	public void onEnable() {
		System.out.println(Mouse.getButtonName(0));
		mc.gameSettings.keyBindAttack.setKeyCode(-99);
		mc.gameSettings.keyBindUseItem.setKeyCode(-100);

		toggle();
	}

}
